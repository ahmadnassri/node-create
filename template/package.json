{
  "private": [project.private],
  "version": "0.0.0-development",
  "name": "[package.name]",
  "description": "[package.description]",
  "author": "[author.name] <[author.email]> ([author.website])",
  "homepage": "https://github.com/[repo.github]/[repo.name]",
  "repository": {
    "type": "git",
    "url": "https://github.com/[repo.github]/[repo.name].git"
  },
  "license": "[project.license]",
  "main": "index.js",
  "keywords": [project.keywords],
  "engines": {
    "node": ">=6"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "bugs": {
    "url": "https://github.com/[repo.github]/[repo.name]/issues"
  },
  "scripts": {
    "test": "npm run test:tap",
    "test:tap": "tap test --100 --coverage-report=lcov --no-browser",
    "coverage": "opener coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "tap": "^11.1.0"
  }
