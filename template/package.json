{
  "private": [private],
  "version": "0.0.0-development",
  "name": "@[org]/[name]",
  "description": "[description]",
  "author": "[author] <[email]> ([website])",
  "homepage": "[homepage]",
  "repository": {
    "type": "git",
    "url": "https://github.com/[org]/[name].git"
  },
  "license": "[license]",
  "main": "index.js",
  "keywords": [keywords],
  "engines": {
    "node": ">=8"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "bugs": {
    "url": "https://github.com/[org]/[name]/issues"
  },
  "scripts": {
    "lint:deps": "npx david --error404 --errorSCM --errorDepType",
    "lint:ec": "npx editorconfig-checker .",
    "lint:js": "npx eslint .",
    "lint:md": "npx remark --quiet --frail .",
    "lint": "npx run-p lint:*",
    "open:coverage": "opener coverage/lcov-report/index.html",
    "test:tap": "tap test --100 --coverage-report=lcov --no-browser",
    "test": "npx run-p test:*"
  },
  "devDependencies": {
    "tap": "^11.1.0"
  }
}
